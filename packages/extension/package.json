{
  "name": "@sireno/extension",
  "version": "0.2.0",
  "private": true,
  "description": "AI-powered assistant for web forms and content. Use your own API keys (OpenAI, Anthropic, Google, Groq) for privacy-first AI assistance on any website.",
  "license": "GPL-3.0",
  "author": "Sergio Carracedo <hi@sergiocarracedo.es>",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "test": "vitest run --exclude test/e2e/**",
    "test:watch": "vitest --exclude test/e2e/**",
    "test:e2e": "playwright test test/e2e/"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "latest",
    "@ai-sdk/google": "latest",
    "@ai-sdk/groq": "^3.0.22",
    "@ai-sdk/openai": "latest",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "ai": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0",
    "use-controllable": "^1.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/chrome": "^0.0.260",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.24",
    "jsdom": "^23.0.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "overrides": {
    "@ai-sdk/provider": "latest"
  }
}
